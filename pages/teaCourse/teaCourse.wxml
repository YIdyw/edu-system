<!--pages/teaCourse/teaCourse.wxml-->

<!-- 导航栏 -->
<i-navigator></i-navigator>

<view class="adver_title">
  <view class="blue_line"></view>
  <view class="adver_title_content">课程信息</view>
</view>

<view class="table">
  <view class="table_content">
    <view class="table_head">
      <view class="head_col">学生</view>
      <view class="head_col">上课数</view>
      <view class="head_col">余课数</view>
    </view>
    <view class="col" wx:for="{{listData}}" wx:key="key">
      <view class="content_col">{{item.col1}}</view>
      <view class="content_col">{{item.col2}}</view>
      <view class="content_col">{{item.col3}}</view>
    </view>
  </view>
</view>

