<!--pages/teaCourse/teaCourse.wxml-->

<!-- 导航栏 -->
<i-navigator></i-navigator>

<view class="pageSapce"  style="top: {{globalData+globalDatas}}px; text-align:center;">
  <!--表格-->
  <text >课程信息</text>
  <view class="divLine"></view>
  <view class="table ">
    <view class="tr thColor">
      <view class="th ">学生</view>
      <view class="th ">上课数</view>
      <view class="th ">余课数</view>
    </view>
    <view class="table-wrap">
      <block wx:for="{{listData}}" wx:key="key">
        <view class="tr">
          <view class="td">{{item.col1}}</view>
          <view class="td">{{item.col2}}</view>
          <view class="td">{{item.col3}}</view>
        </view>
      </block>
    </view>
  </view>
</view>
