<!-- 导航栏 -->
<i-navigator></i-navigator>

<view class="pageSapce"  style="top: {{globalData+globalDatas}}px;">
  <!--表格-->
  <text>课程信息</text>
  <view class="divLine"></view>
  <view class="table ">
    <view class="tr thColor">
      <view class="th">属性</view>
      <view class="th ">数据值</view>
    </view>
    <view class="table-wrap">
      <block wx:for="{{listData}}" wx:key="{{key}}">
        <view class="tr">
          <view class="td">{{item.col1}}</view>
          <view class="td">{{item.col2}}</view>
        </view>
      </block>
    </view>
  </view>
</view>

