/* component/navigator/navigator.wxss */

.navbar{
  background: #2655CF;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  }

  .title-container {
  height: 40px;
  display: flex;
  align-items: center;
  position: relative;
  }
  .capsule {
  margin-left: 10px;
  height: 30px;
  border-radius: 16px;
  display: flex;
  align-items: center;
  }
  
  .back {
  width: 45px;
  height: 60%;
  position: relative;
  }

  .home {
    border-left: 1px solid #2655CF;
  }
  
  .image {
    width: 50%;
    height: 100%;
    position: absolute;
    left: 0%;
    top: 50%;
    transform: translate(-50%,-50%);
  }

  .title {
  color: white;
  position: absolute;
  top: 6px;
  left: 104px;
  right: 104px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  }

  .navigator{
    position: fixed;
    top: 0px;
    z-index: 2;
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    opacity: 1;
  }
  .navigator1{
    background: #2655CF;
  }
  .navigator2{
    background: #2655CF;
    display: flex;
    flex-direction: row;
  }
  .navigator3{
    position: absolute;
    left: 30rpx;
    font-size: 26px;
    font-family: Microsoft YaHei;
    font-weight: 300;
    color: #FFFFFF;
    line-height: 48px;
    border-radius: 2px;
  }